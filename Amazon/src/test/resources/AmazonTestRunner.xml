<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE suite SYSTEM "http://testng.org/testng-1.0.dtd">

<suite name ="Automation Practice Test Script" parallel="false">

    <parameter name="useCloudEnv" value="true"/>
    <parameter name="cloudEnvName" value="browserstack"/>
    <parameter name="os" value="Windows"/>
    <parameter name="os_version" value="10"/>
    <parameter name="browserName" value="Chrome"/>
    <parameter name="browserVersion" value="84.0"/>
    <parameter name="url" value="http://www.amazon.com/"></parameter>

    <test name = "Automation Practice Home">

        <classes>
            <class name="Search.SearchItems" />
            <class name="Search.DropDownMenu" />
        </classes>

    </test>

</suite>
